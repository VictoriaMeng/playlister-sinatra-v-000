<%= @song.name %>
<a href="/artists/<%= @song.artist.slug %>"><%= @song.artist.name %></a>
<% @song.artist.genres.map do |genre| %>
  <a href="/genres/<%= genre.slug %>"><%= genre.name %></a>
<% end %>
<% if flash.has?(:message) %>
  <%= flash[:message] %>
  <%= @song.name %>
  <%= @song.artist.name %>
<% end %>
